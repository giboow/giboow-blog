{
  "plugins": [
    [
      "component",
      "transform-async-to-generator",
      "transform-flow-strip-types",
      "transform-es2015-destructuring",
    ],
    [
      "module-resolver", {
      "root": ["."],
      "alias": {
        "styles": "./styles"
      },
      "cwd": "babelrc"
    }],
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ]
  ],
  "presets": [
    "next/babel",
    "es2015",
    "stage-2"
  ],
  "ignore": []
}